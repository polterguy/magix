


inspect=>@"creates the default settings for your portal"
magix.data.load
  id=>magix.admin.settings.default-front-end-css-file
if=>not-exist
  lhs=>[magix.data.load][objects]
  code
    magix.data.save
      id=>magix.admin.settings.default-front-end-css-file
      value
        file=>media/front-end/hipster.css
    magix.data.save
      value
        type=>magix.admin.setting.show-help
        value=>true
