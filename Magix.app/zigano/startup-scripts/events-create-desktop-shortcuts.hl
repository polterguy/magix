

event=>magix.admin.desktop-shortcuts.defaults

  inspect=>@"returns all the default desktop keyboard 
shortcuts in the system.&nbsp;&nbsp;thread safe"


  code
    _scripts
      _default_script4
        text=>overridden active events
        category=>security
        code
          magix.admin.run-file
            file=>zigano/admin/tools/inspect-overridden-events.hl
          magix.viewport.show-message-if-help-mode
            message=>@"view, inspect and run any overridden active event 
in magix illuminate"
      _default_script2
        text=>unit tests
        category=>tools
        code
          magix.admin.run-file
            file=>zigano/admin/tools/inspect-unit-tests.hl
          magix.viewport.show-message-if-help-mode
            message=>@"view, inspect and run unit tests
in magix, which is an integral part of the system itself.&nbsp;&nbsp;
magix illuminate is largely literate, according to donald knuth's
definition.&nbsp;&nbsp;here you can inspect and run individual unit tests,
or all unit tests in the system"
      _default_script3
        text=>active events
        category=>tools
        code
          magix.admin.run-file
            file=>zigano/admin/tools/inspect-events.hl
          magix.viewport.show-message-if-help-mode
            message=>@"view, inspect and run any active event 
in magix, which is an integral part of the system itself.&nbsp;&nbsp;
magix illuminate is largely literate, according to donald knuth's
definition, or self-documenting with other words"
      _default_script17
        text=>pages
        category=>applications
        code
