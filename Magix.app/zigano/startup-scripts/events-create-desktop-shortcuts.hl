      _default_scriptx_4
        text=>logout
        category=>security
        code
          magix.web.set-session
            key=>zigano.travel.user
          magix.viewport.execute-javascript
            script=>@"location.reload();"
      _default_script7
        text=>magix markup language editor
        category=>tools
        code
          magix.admin.run-file
            file=>zigano/admin/tools/magix-markup-editor.hl
          magix.viewport.show-message-if-help-mode
            message=>@"magix illuminate contains a markup
language, which is a superset of html, where you can 
mix in server-side hyper lisp controls by opening
a server-side web-form using two braces around you server-side 
node-structure hierarchy for rendering server-side web controls.&nbsp;&nbsp;
use the mml editor to edit and play around with magix markup language, 
and see the results immediately in a preview window"
      _default_script9
        text=>turn on tracer
        category=>tools
        code
          magix.viewport.load-module
            container=>trace
            css=>span-24 top-3 bottom-3
            name=>Magix.admin.EventSniffer
          magix.viewport.show-message-if-help-mode
            message=>@"turn tracing on in 
magix, such that you can see every single active
event raised on the server, in both the before version,
and in the after version.&nbsp;&nbsp;
very good for teaching purposes, to understand how
magix is tied together, such that you can start 
modifying it yourself"
      _default_script12
        text=>turn off tracer
        category=>tools
        code
          magix.viewport.clear-controls
            container=>trace
          magix.viewport.show-message-if-help-mode
            message=>@"turn tracing off in 
magix, meaning it unloads the trace module, 
if loaded.&nbsp;&nbsp; see turn on tracer"
      _default_scriptxx9
        text=>turn on profiler
