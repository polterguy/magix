

inspect=>@"creates unit tests for testing if 
viewstate events functions"


event=>magix.test.viewstate-set-retrieve-delete

  inspect=>@"creates the unit test
necessary to test if viewstate retrieve and set works correctly"

  code
    magix.viewport.set-viewstate=>magix.test.test-viewstate
      value=>howdy
        child1=>howdy2
    magix.viewport.get-viewstate=>magix.test.test-viewstate
    if=>not-exist
      lhs=>[magix.viewport.get-viewstate][value]
      code
        throw=>couldn't find viewstate object
    if=>not-equals
      lhs=>[magix.viewport.get-viewstate][value].Value
      code
        throw=>couldn't find correct value for session object
    if=>not-equals
      lhs=>[magix.viewport.get-viewstate][value][child1].Value
      rhs=>howdy2
      code
        throw=>couldn't find correct value for session child node object
    magix.viewport.set-viewstate=>magix.test.test-viewstate
    magix.viewport.get-viewstate=>magix.test.test-viewstate
    if=>exist
      lhs=>[magix.viewport.get-viewstate:1][value]
      code
        throw=>couldn't delete viewstate object
