

inspect=>@"creates unit tests for testing execution 
of files"


event=>magix.test.execute-file-with-parameters

  inspect=>@"verifies that running a file 
with parameters, both input and output, is behaving
correctly"

  code
    execute-file=>system42/admin/test-files/test-input-pars.hl
      mumbo=>howdy
    if=>not-equals
      lhs=>[execute-file][mumbo].Value
      rhs=>howdy
      code
        throw=>input value not returned
    if=>not-equals
      lhs=>[execute-file][jumbo].Value
      rhs=>5
      code
        throw=>output value not returned currectly

