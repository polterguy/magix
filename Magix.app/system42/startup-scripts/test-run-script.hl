

inspect=>@"creates unit tests for testing execution 
of inline script"


event=>magix.test.execute-script-with-parameters

  inspect=>@"verifies that running a script 
with parameters, both input and output, is behaving
correctly"

  code
    execute-script=>@"
if=>[$][mumbo].Value!=howdy
  throw=>input parameter not accessible in file
set=>[$][jumbo].Value
  value=>5"
      mumbo=>howdy
    if=>[execute-script][mumbo].Value!=howdy
      throw=>input value not returned
    if=>[execute-script][jumbo].Value!=5
      throw=>output value not returned currectly

