Method:magix.execute

inspect=>@"Will show a screen of all the 
active event in the system, and allow
the user to inspect them by clicking
them. Does not show private active events, 
meaning active events starting wiith '_'."

magix.viewport.clear-controls
  container=>content
magix.forms.create-web-page
  container=>header
  html=>@"<h1>active events</h1>"
  form-id=>header
magix.admin.get-active-events
  all=>False
set=>[magix.admin.get-active-events].Name
  value=>Data
for-each=>[Data][ActiveEvents]
  Tpl
    LinkButton
      CssClass=>span4 btn btn-small btn-left
      Text
      OnClick
        raise=>
          inspect
        set=>[2][value].Value
          value=>[0][inspect].Value
        magix.forms.set-value
          form-id=>form1
          id=>desc
          value
        set=>[4][value].Value
          value=>[0].Value
        magix.forms.set-value
          form-id=>form1
          id=>header
          value
        set=>[0][inspect]
        set=>[magix.code.node-2-code][JSON].Value
          value=>[0]
        magix.code.node-2-code
          JSON
        set=>[9][value].Value
          value=>[magix.code.node-2-code][code].Value
        magix.forms.set-value
          form-id=>form1
          id=>code
          value
        set=>[0][inspect].Name
          value=>inspect
  set=>[.ip][..][Tpl][LinkButton][OnClick][0].Value
    value=>[.].Value
  set=>[.ip][..][Tpl][LinkButton][Text].Value
    value=>[.].Value
  add=>[/][cf][controls][Panel][controls]
    value=>[.ip][..][Tpl][LinkButton]
set=>[cf][controls][Panel2].Name
  value=>Panel
set=>[cf].Name
  value=>magix.forms.create-form
cf
  form-id=>form1
  container=>content
  controls
    Panel2
      CssClass=>span5
      controls
        Label=>header
          Text=>name of active event
          CssClass=>span4
          Tag=>strong
        Label=>desc
          CssClass=>span4
          Text=>@"description of your active event.&nbsp;&nbsp;
click any of the active events to the right to inspect them"
        Label=>code
          CssClass=>span4
          Text=>code example ...
          Tag=>pre
        LinkButton=>run
          Text=>inspect
          CssClass=>btn btn-info btn-block-level span1 offset3
          OnClick
            magix.forms.get-value
              form-id=>form1
              id=>code
            set=>[.ip][..][magix.admin.load-executor-code][code].Value
              value=>[.ip][..][magix.forms.get-value][value].Value
            magix.admin.load-executor-code
              code
    Panel=>wrp
      CssClass=>span3
      controls
