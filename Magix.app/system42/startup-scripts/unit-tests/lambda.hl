

inspect=>@"creates unit tests to 
test the [magix.execute.lambda] keyword"


event
  name=>magix.test.lambda
  persist=>false
  inspect=>@"asserts that [lambda] works the 
way it should work, and that it allows for 
passing parameters in and out of the expression.&nbsp;&nbsp;
it also asserts that the code executed is copied, 
and that not the original code is changed from 
within the [lambda] object"
  code
    _data
      set=>[/][_output].Value
        value=>not shown
      set=>[$][output].Value
        value=>{0} {1}
          =>[$][input].Value
          =>world
    lambda=>[_data]
      input=>howdy
    if=>not-equals
      lhs=>[lambda][output].Value
      rhs=>howdy world
      code
        throw=>[lambda] did not return values as expected
    if=>exist
      lhs=>[_data][_output]
      code
        throw=>@"[lambda] modified expression's node, 
which it should not"
    if=>exist
      lhs=>[_output]
      code
        throw=>@"[lambda] modified outside expression's node, 
which it should not"

